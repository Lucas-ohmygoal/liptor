/******QToolButton******/

QToolButton
{
    border: 0px solid #b1b1b1;
    color:#b1b1b1;
    margin: 1px 1px 1px 1px;
}
QToolButton:hover
{
    background: #2e3135;
    color:#ffffff;
}

QToolButton:checked
{
    background: #9ACD32;
    color:#ffffff;
}

QToolButton:checked:hover
{
    background: #7aad12;
    color:#ffffff;
}
QToolTip
{
    background: #2e3135;
    opacity:255;
    border: 1px solid #7a7a7a;
    color:#b1b1b1;
}

QToolButton#exit_button:hover{

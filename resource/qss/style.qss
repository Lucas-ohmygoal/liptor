/******QPushButton******/

QPushButton#connect_sign {
    font-size: 50px;
    background: transparent;
    color: white;
}

QPushButton#connect_sign:hover {
    font-size: 60px;
    font-weight: 500;
    border-bottom-color: #b2f37e;
    background: transparent;
    color: #b2f37e;
}

QPushButton
{
    background: #74787c;
    opacity : 255;
    border - radius: 5;
    color: #f6f5ec;
}

QPushButton:hover
{
    background: #64686c;
    color: #ffffff;
}

QPushButton:checked
{
    background: #9acd32;
    color: #ffffff;
}

QPushButton:checked:hover
{
    background: #8abd22;
    color:#ffffff;
}

QPushButton::menu-indicator
{
    image: url(../images/down_arrow.png);
    padding: 2px 2px 0px 0px;
    subcontrol-position: right center;
    subcontrol-origin: padding;
}

QPushButton::menu-indicator:hover
{
    image: url(../images/down_arrow.png);
    padding: 2px 2px 0px 0px;
    subcontrol-position: right center;
    subcontrol-origin: padding;
}

QPushButton::menu-indicator:!enabled
{
    image: url(../images/down_arrow.png);
    padding: 2px 2px 0px 0px;
    subcontrol-position: right center;
    subcontrol-origin: padding;
}

QPushButton:!enabled
{
    background: #3e4145;
    color: #535355;
    border: 1px solid #535355;
}

/**********QLabel***********/
QLabel
{
    background: transparent;
    color: white;
    font-size: 40px;
    text-align: center;
}

QLabel#joint_number
{
    background: transparent;
    color: white;
    font-size: 55px;
    text-align: center;
}

QLabel#error_label
{
    background: transparent;
    color: red;
    font-size: 40px;
    text-align: center;
}
QLabel#error_board
{
    background: qradialgradient(cx:1, cy:1, radius:1, fx:0.7, fy:0.7, stop:0 #255190, stop:1 #3572a6, stop:2 #5fc0d5);
    color: #e4866b;
    font-size: 30px;
    text-align: center;
    border-radius: 10px;
}


QLabel#TitleContent,#TitleIcon
{
    color: #b1b1b1;
    background: transparent;
}

QLabel#cam_box_i, #map_box_i, #cam_box_a, #map_box_a, #cam_box_r, #map_box_r, #cam_box_c
{
    background: transparent;
    border-width: 2px;
    border-color: #1e4c7c;
}
/*******QLineEdit*********/

QLineEdit{
    border-color: transparent;
    margin-top: 10px;
    padding-left: 5px;
    padding-right: 5px;
    border-radius: 5px;
    width: 200px;
    height: 60px;
    font-size: 40px;
    color: #12243a;
    font-weight: 600;
    background-color: #b5d7e4;
}

QLineEdit:!enabled
{
    background: #626262;
    color: #bebebe;
}


/*******QSlider*******/

QSlider:vertical{
	padding-left: 15px;
	padding-right: 15px;
	border-radius: 5px;
	width:50px;
	height: 210px;
}

QSlider:horizontal{
	background-color: 64C2E5;
	padding-top: 15px;
	padding-bottom: 15px;
	border-radius: 5px;
}

QSlider::add-page:vertical {
	background-color: #6aa8e1;
	width:7px;
	border-radius: 6px;
}

QSlider::sub-page:vertical {
	background: #c1c0c0;
	width:5px;
	border-radius: 10px;
}

QSlider::groove:vertical {
	background: #c1c0c0;
	width:7px;
	top: 0px;
    bottom: 0px;
    border:0px;
	border-radius:10px;
	height:210px;
}

QSlider::handle:vertical {
	height: 50px;
	width: 50px;
	margin-top: 0px;
    margin-left: -20px;
    margin-bottom: 0px;
    margin-right: -20px;
}
QSlider::handle:vertical:pressed {
	height: 45px;
	width: 45px;

}

QSlider::groove:horizontal{
        height: 12px;
        left: 0px;
        right: 0px;
        border:0px;
        border-radius:6px;
        background: #c1c0c0;
 }
 QSlider::handle:horizontal{
        width:  50px;
        height: 50px;
        margin-top: -20px;
        margin-left: 0px;
        margin-bottom: -20px;
        margin-right: 0px;
}

QSlider::handle:horizontal:pressed{
        width:  45px;
        height: 45px;
}

QSlider::sub-page:horizontal{
       background: #6aa8e1;
       border-radius:6px;
}

/**************QComboBox*************/

QComboBox
{
	border-radius:15px;
	background-color: #bbc1c2;
	font: 75 20pt "Microsoft YaHei";
	font-weight: 500;
    color: #282828;
    border:0px ;
	padding-top: 2px;
	padding-left: 2px;
	padding-bottom: 2px;
	width: 260px;
}
QComboBox:disabled
{
	background-color:rgba(50,50,50,200);
	font: 75 20pt "Microsoft YaHei";
    color:rgb(160,160,160);
}
QComboBox:hover
{
	background-color:#bbc1c2;
	border:1px solid rgb(31,156,220) ;
	border-radius:15px;
}

QComboBox:on
{
    border-top-left-radius: 15px;
    border-top-right-radius: 15px;
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
	background-color:#bbc1c2;
	font: 75 20pt "Microsoft YaHei";
    color:#282828;
    border:1px solid rgb(31,156,220) ;
}

QComboBox QAbstractItemView
{

    outline: 0px solid gray;
    border: 1px solid rgb(31,156,220);
    font: 75 20pt "Microsoft YaHei";
    color: #282828;
    background-color: #bbc1c2;
    selection-background-color: rgb(90,90,90);
}

QComboBox QAbstractItemView::item
 {
	height: 25px;
	border-radius:15px;
 }

QComboBox QAbstractItemView::item:selected
{
    color: #282828;
	background-color: #bbc1c2;
	border-radius:15px;
}

QComboBox::drop-down
{
	border-left-width: 1px;
    border-left-style: solid;
    border-bottom-left-radius: 15px;
    border-bottom-right-radius: 15px;
}

QComboBox::down-arrow
{
    right:10px;
    width: 30px;
    height: 30px;
    image: url(:/resource/images/arrow-up.png);
}

QComboBox::down-arrow:on
{
    width: 30px;
    height: 30px;
    image: url(:/resource/images/arrow-down.png);
}

/******QWidget******/

QWidget#connector_pane {
    border-radius: 15px;
}

QWidget#menu_bar {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0.8 #000000 stop:0.6 #020a13, stop:0.4 #031121, stop:0.2 #06182c);
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#top_box {
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#top_box_s {
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#bot_box_s {
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#bot_box {
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#center_box, #ing_box, #arm_box, #car_box, #cam_box{
    background-color: qradialgradient(cx:1, cy:1, radius:1, fx:0.7, fy:0.7, stop:0 #000305, stop:1 #0c273f, stop:2 #1a4461);
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget#int_box, #set_box{
    background-color: qradialgradient(cx:1, cy:1, radius:1, fx:0.7, fy:0.7, stop:0 #000305, stop:1 #0c273f, stop:2 #1a4461);
    border: 0px solid #00ff00;
    color: #b1b1b1;
}

QWidget {
    border: 0px solid #00ff00;
    color: white;
}

QWidget#bot_box_i, #bot_box_a, #bot_box_r, #bot_box_c {
    background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1, stop:0 #212121, stop:0.5 #1a3d71, stop:1 #212121);
    padding: 0px; margin: 0px;
    border-width: 3px;
    border-color: #1e4c7c;
    border-radius: 30px;
}


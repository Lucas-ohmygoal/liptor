    background: #e65541;
    border-radius: 15px;
    margin: 0px 1px 1px 1px;
    height:20;
}

QToolButton#exit_button:!hover{
    border: 0px solid #b1b1b1;
    border-radius: 15px;
    height:20;
}

QToolButton#maximize_button:hover
{
    border: 0px solid #b1b1b1;
